<!-- Important: in Gary wizard  (Step 2 - information step, not including schedule) -->

<ul id="breadcrumb">
	<li><%= link_to "Step 1 > Login Info", edit_user_registration_path %></li> 
	<li class="active">Step 2 > A Bit About You</li>
	<li class="faded">Step 3 > Availability</li>
</ul>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :method => :put, :multipart => true }) do |f| %>

	<%= f.error_messages %>
